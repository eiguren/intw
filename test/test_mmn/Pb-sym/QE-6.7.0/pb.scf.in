&control
    calculation='scf'
    restart_mode='from_scratch',
    prefix='pb',
    pseudo_dir = './',
    outdir='./'
    wf_collect = .true.
    tprnfor = .true.,
    tstress = .true.,
/
&system
    ibrav=  2, 
    celldm(1) = 9.193088994,
    nat= 1,
    ntyp= 1,
    ecutwfc = 80.0
    occupations='smearing',
    smearing='marzari-vanderbilt',
    degauss=0.05,
    nbnd=15,
    noncolin=.false.,
    lspinorb=.false.
/
&electrons
    conv_thr =  1.0d-12
    mixing_beta = 0.7
/
ATOMIC_SPECIES
    Pb 207.2 pb_s.UPF
ATOMIC_POSITIONS
    Pb 0.00 0.00 0.00
K_POINTS {automatic}
    6 6 6  0 0 0
